    "@types/jest": "^29.5.0",
    "@types/node": "^20.14.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "~5.5.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
