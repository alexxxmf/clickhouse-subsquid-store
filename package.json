  "keywords": [
    "subsquid",
    "clickhouse",
    "blockchain",
    "indexer",
    "evm",
    "database",
    "hot-cold",
    "reorg-handling"
  ],
  "author": "Alex Moro Fernandez",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexxxmf/clickhouse-subsquid-store.git"
  },
  "bugs": {
    "url": "https://github.com/alexxxmf/clickhouse-subsquid-store/issues"
  },
  "homepage": "https://github.com/alexxxmf/clickhouse-subsquid-store#readme",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
